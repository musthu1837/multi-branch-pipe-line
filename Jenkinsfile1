    agent any

    stages {
        stage("Build"){
            steps{
                script{
                    buildSystems.mavenBuild()
                }
            }
        }
        stage("Dev") {
            steps {
                echo "Started stage Dev"
            }
        }

        stage("Prod") {
            steps {
                echo "Started stage prod"
            }
        }
    }
}
